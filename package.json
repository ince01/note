{
  "name": "note",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@apollo/client": "^3.3.13",
    "@craco/craco": "^6.1.1",
    "@hookform/resolvers": "^1.3.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "antd": "^4.12.3",
    "craco-less": "^1.17.1",
    "graphql": "^15.5.0",
    "node-sass": "^5.0.0",
    "postcss-import": "^14.0.1",
    "postcss-nested": "^5.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.5",
    "react-intl": "^5.12.5",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-streams": "^5.1.3",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "postinstall": "husky install",
    "extract-messages:en": "formatjs extract 'src/**/*.tsx' --out-file src/translations/locales/en_US.extracted.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "extract-messages:vi": "formatjs extract 'src/**/*.tsx' --out-file src/translations/locales/vi_VN.extracted.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-messages:en": "formatjs compile src/translations/locales/en_US.extracted.json --ast --out-file src/translations/locales/en_US.json",
    "compile-messages:vi": "formatjs compile src/translations/locales/vi_VN.extracted.json --ast --out-file src/translations/locales/vi_VN.json",
    "build-messages:dev": "yarn extract-messages:en && yarn extract-messages:vi && yarn compile-messages:en && yarn compile-messages:vi",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^3.1.3",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@types/marked": "^1.2.2",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^9.8.6",
    "babel-plugin-formatjs": "^9.1.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.0",
    "lint-staged": "^10.5.4",
    "postcss": "^7.0.35",
    "prettier": "^2.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
  }
}
